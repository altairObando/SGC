            @Html.DisplayFor(modelItem=> item.Documentos)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idProceso }) |
            @Html.ActionLink("Details", "Details", new { id=item.idProceso }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idProceso })
