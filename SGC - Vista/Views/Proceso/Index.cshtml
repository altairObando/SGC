        <td>
            @Html.DisplayFor(modelItem => item.EstadoGestion.estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fechaFin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.anulado)
        </td>
