@model IEnumerable<SGC___Modelo.Proceso>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EstadoGestion.estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fechaInicio)
        </th>
        <th>
            Fecha Fin
            @*@Html.DisplayNameFor(model => model.fechaFin)*@
        </th>
        <th>
            @Html.DisplayNameFor(model => model.anulado)
        </th>
        <th>Documentos</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
